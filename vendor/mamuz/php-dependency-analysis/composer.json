{
    "name"             : "mamuz/php-dependency-analysis",
    "type"             : "project",
    "description"      : "Static code analysis to provide and verify a dependency graph against a defined architecture",
    "homepage"         : "https://github.com/mamuz/PhpDependencyAnalysis",
    "license"          : "MIT",
    "keywords"         : [
        "phpda", "dependency", "analysis", "graph", "architecture"
    ],
    "authors"          : [
        {
            "name" : "Marco Muths",
            "email": "mamuz.de@gmail.com"
        }
    ],
    "support"          : {
        "issues": "https://github.com/mamuz/PhpDependencyAnalysis/issues",
        "source": "https://github.com/mamuz/PhpDependencyAnalysis"
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "require"          : {
        "php"                              : ">=5.3.3",
        "psr/log"                          : "~1.0",
        "nikic/php-parser"                 : "~1.3",
        "clue/graph"                       : "~0.8",
        "graphp/algorithms"                : "~0.8.1",
        "graphp/graphviz"                  : "~0.2.0",
        "phpdocumentor/reflection-docblock": "~2.0.1",
        "doctrine/collections"             : "~1.0",
        "symfony/yaml"                     : "~2.5|~3.0",
        "symfony/console"                  : "~2.5|~3.0",
        "symfony/finder"                   : "~2.5|~3.0"
    },
    "require-dev"      : {
        "phpunit/phpunit"       : "~4.0",
        "mockery/mockery"       : "~0.9.2"
    },
    "autoload"         : {
        "psr-4": {
            "PhpDA\\": "src/"
        }
    },
    "bin"              : ["bin/phpda"]
}
