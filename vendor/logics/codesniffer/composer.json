{
    "name": "logics/codesniffer",
    "keywords": ["php", "code", "sniffer", "linter", "static", "analysis"],
    "description": "PHP CodeSniffer static analysis tool",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Bashkirtsev",
            "email": "vladimir@bashkirtsev.com",
            "homepage": "http://www.bashkirtsev.com",
            "role": "Developer"
        }
    ],
    "bin": ["src/phpcs"],
    "autoload" : {
        "psr-4" : {
            "Logics\\BuildTools\\CodeSniffer\\": [
                "src",
                "src/CodeSniffer",
                "src/CodeSniffer/Reports",
                "src/CodeSniffer/Standards",
                "src/CodeSniffer/Tokenizers"
            ],
            "Logics\\BuildTools\\CodeSniffer\\Beauty\\": [
                "src/CodeSniffer/Standards/Beauty/Sniffs/Classes",
                "src/CodeSniffer/Standards/Beauty/Sniffs/Commenting",
                "src/CodeSniffer/Standards/Beauty/Sniffs/Constants",
                "src/CodeSniffer/Standards/Beauty/Sniffs/ControlStructures",
                "src/CodeSniffer/Standards/Beauty/Sniffs/Functions",
                "src/CodeSniffer/Standards/Beauty/Sniffs/NamingConventions",
                "src/CodeSniffer/Standards/Beauty/Sniffs/PHP",
                "src/CodeSniffer/Standards/Beauty/Sniffs/Strings",
                "src/CodeSniffer/Standards/Beauty/Sniffs/WhiteSpace"
            ],
            "Logics\\BuildTools\\CodeSniffer\\Generic\\": [
                "src/CodeSniffer/Standards/Generic/Sniffs/Arrays",
                "src/CodeSniffer/Standards/Generic/Sniffs/Classes",
                "src/CodeSniffer/Standards/Generic/Sniffs/CodeAnalysis",
                "src/CodeSniffer/Standards/Generic/Sniffs/Commenting",
                "src/CodeSniffer/Standards/Generic/Sniffs/ControlStructures",
                "src/CodeSniffer/Standards/Generic/Sniffs/Debug",
                "src/CodeSniffer/Standards/Generic/Sniffs/Files",
                "src/CodeSniffer/Standards/Generic/Sniffs/Formatting",
                "src/CodeSniffer/Standards/Generic/Sniffs/Functions",
                "src/CodeSniffer/Standards/Generic/Sniffs/Metrics",
                "src/CodeSniffer/Standards/Generic/Sniffs/NamingConventions",
                "src/CodeSniffer/Standards/Generic/Sniffs/PHP",
                "src/CodeSniffer/Standards/Generic/Sniffs/Strings",
                "src/CodeSniffer/Standards/Generic/Sniffs/VersionControl",
                "src/CodeSniffer/Standards/Generic/Sniffs/WhiteSpace"
            ],
            "Logics\\BuildTools\\CodeSniffer\\MySource\\": [
                "src/CodeSniffer/Standards/MySource/Sniffs/CSS",
                "src/CodeSniffer/Standards/MySource/Sniffs/Channels",
                "src/CodeSniffer/Standards/MySource/Sniffs/Commenting",
                "src/CodeSniffer/Standards/MySource/Sniffs/Debug",
                "src/CodeSniffer/Standards/MySource/Sniffs/Objects",
                "src/CodeSniffer/Standards/MySource/Sniffs/PHP",
                "src/CodeSniffer/Standards/MySource/Sniffs/Strings"
            ],
            "Logics\\BuildTools\\CodeSniffer\\PEAR\\": [
                "src/CodeSniffer/Standards/PEAR/Sniffs/Classes",
                "src/CodeSniffer/Standards/PEAR/Sniffs/Commenting",
                "src/CodeSniffer/Standards/PEAR/Sniffs/ControlStructures",
                "src/CodeSniffer/Standards/PEAR/Sniffs/Files",
                "src/CodeSniffer/Standards/PEAR/Sniffs/Formatting",
                "src/CodeSniffer/Standards/PEAR/Sniffs/Functions",
                "src/CodeSniffer/Standards/PEAR/Sniffs/NamingConventions",
                "src/CodeSniffer/Standards/PEAR/Sniffs/WhiteSpace"
            ],
            "Logics\\BuildTools\\CodeSniffer\\PSR1\\": [
                "src/CodeSniffer/Standards/PSR1/Sniffs/Classes",
                "src/CodeSniffer/Standards/PSR1/Sniffs/Files",
                "src/CodeSniffer/Standards/PSR1/Sniffs/Methods"
            ],
            "Logics\\BuildTools\\CodeSniffer\\PSR2\\": [
                "src/CodeSniffer/Standards/PSR2/Sniffs/Classes",
                "src/CodeSniffer/Standards/PSR2/Sniffs/ControlStructures",
                "src/CodeSniffer/Standards/PSR2/Sniffs/Files",
                "src/CodeSniffer/Standards/PSR2/Sniffs/Methods",
                "src/CodeSniffer/Standards/PSR2/Sniffs/Namespaces"
            ],
            "Logics\\BuildTools\\CodeSniffer\\Squiz\\": [
                "src/CodeSniffer/Standards/Squiz/Sniffs/Arrays",
                "src/CodeSniffer/Standards/Squiz/Sniffs/CSS",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Classes",
                "src/CodeSniffer/Standards/Squiz/Sniffs/CodeAnalysis",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Commenting",
                "src/CodeSniffer/Standards/Squiz/Sniffs/ControlStructures",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Debug",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Files",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Formatting",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Functions",
                "src/CodeSniffer/Standards/Squiz/Sniffs/NamingConventions",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Objects",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Operators",
                "src/CodeSniffer/Standards/Squiz/Sniffs/PHP",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Scope",
                "src/CodeSniffer/Standards/Squiz/Sniffs/Strings",
                "src/CodeSniffer/Standards/Squiz/Sniffs/WhiteSpace"
            ],
            "Logics\\BuildTools\\CodeSniffer\\Zend\\": [
                "src/CodeSniffer/Standards/Zend/Sniffs/Debug",
                "src/CodeSniffer/Standards/Zend/Sniffs/Files",
                "src/CodeSniffer/Standards/Zend/Sniffs/NamingConventions"
            ]
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-SimpleXML": "*",
        "ext-gettext": "*",
        "ext-tokenizer": "*",
        "ext-iconv": "*",
        "ext-xmlwriter": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "phpunit/php-timer": "^1.0"
    },
    "autoload-dev" : {
        "psr-4" : {
            "Logics\\Tests\\BuildTools\\CodeSniffer\\": [
                "tests",
                "tests/Core/Reports",
                "tests/Core/Reports/Mock",
                "tests/Standards"
            ]
        }
    },
    "require-dev": {
        "ext-runkit": "*",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "phploc/phploc": "^2.1",
        "sebastian/phpcpd": "^2.0",
        "phpdocumentor/phpdocumentor": "^2.9",
        "mamuz/php-dependency-analysis": "^0.6",
        "phpunit/phpunit": "^4.8",
        "logics/phpunit-extensions": "^0.2||dev-trunk",
        "logics/configgenerator": "^0.1||dev-trunk",
        "logics/extcheck": "^0.1||dev-trunk"
    },
    "prefer-stable": true
}
