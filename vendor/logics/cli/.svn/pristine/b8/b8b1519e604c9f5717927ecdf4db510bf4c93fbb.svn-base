{
    "name": "logics/cli",
    "keywords": ["CLI", "command", "line", "interface", "cron"],
    "description": "Command Line Interface base class providing command line parsing facilities",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Bashkirtsev",
            "email": "vladimir@bashkirtsev.com",
            "homepage": "http://www.bashkirtsev.com",
            "role": "Developer"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Logics\\Foundation\\CLI\\": ["src"]
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-gettext": "*",
        "ext-posix": "*",
        "ext-mbstring": "*"
    },
    "autoload-dev" : {
        "psr-4" : {
            "Logics\\Tests\\Foundation\\CLI\\": ["tests"]
        }
    },
    "require-dev": {
        "ext-test_helpers": "*",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "phploc/phploc": "^2.1",
        "sebastian/phpcpd": "^2.0",
        "phpdocumentor/phpdocumentor": "^2.9",
        "mamuz/php-dependency-analysis": "^0.6",
        "phpunit/phpunit": "^5.7",
        "logics/phpunit-extensions": "^0.2||dev-trunk",
        "logics/codesniffer": "^0.1||dev-trunk"
    },
    "prefer-stable": true
}
